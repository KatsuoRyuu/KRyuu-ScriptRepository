    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
	"Script Repository"
    ],
    "homepage": "https://github.com/KatsuoRyuu/KRyuuScriptRepository",
    "authors": [
	{
	    "name": "Anders Blenstrup-Pedersen",
	    "email": "anders-github@drake-development.org",
	    "homepage": "http://drake-development.org",
	    "role": "Author"
	}
    ],
    "require": {
	"php": ">=5.3.3",
	"zendframework/zendframework": "2.*",
	"doctrine/doctrine-module" : "~0.7",
	"doctrine/doctrine-orm-module" : "~0.7"
    },
    "autoload": {
        "psr-0": {
            "KRyuu-ScriptRepository": "src/"
        },
        "classmap": [
